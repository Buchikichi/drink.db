CREATE TABLE `item` (
  `itemId` char(32) COLLATE utf8_bin NOT NULL COMMENT 'アイテムID',
  `kindId` char(32) COLLATE utf8_bin NOT NULL COMMENT '種類ID',
  `countryCd` char(3) COLLATE utf8_bin NOT NULL COMMENT '国名コード',
  `noteId` char(32) COLLATE utf8_bin NOT NULL COMMENT '名称',
  `synonym` text COLLATE utf8_bin NOT NULL COMMENT '同義語',
  `thumbnail` text COLLATE utf8_bin COMMENT 'サムネイル',
  `imgsrc` mediumtext COLLATE utf8_bin COMMENT 'イメージ',
  PRIMARY KEY (`itemId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin COMMENT='アイテム'